---
import "../styles/style.css";
import "../fonts/Rubik.css";

interface Props {
    title: string;
}

const { title } = Astro.props;
---

<meta charset="UTF-8" />
<meta http-equiv="X-UA-Compatible" content="IE=edge" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />

<meta name="theme-color" content="#7e4895" />
<meta property="og:site_name" content="Kierownik223's website" />
<meta property="og:description" content="Kierownik223's website" />
<meta property="og:type" content="website" />
<meta name="keywords" content="kierownik kierownik223" />
<meta property="og:locale" content="en" />
<meta property="og:title" content={title} />
<meta name="description" content="Kierownik223's website" />

<link rel="apple-touch-icon" href="favicon.svg">
<link rel="shortcut icon" href="favicon.svg" type="image/x-icon">
<link rel="icon" href="favicon.svg">

<title>{title} - Kierownik's site</title>
